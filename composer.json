{
    "name": "uccello/record-manager",
    "description": "A complete CRUD solution to manage records in a Laravel project.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonathan SARDO",
            "email": "jonathan@uccellolabs.com"
        }
    ],
    "require": {
        "livewire/livewire": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "Uccello\\RecordManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Uccello\\RecordManager\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Uccello\\RecordManager\\Providers\\AppServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
